{
  "name": "personal-site",
  "description": "Zach Lysobey's personal site",
  "version": "1.0.0",
  "author": "Zach Lysobey <zlysobey+dev@gmail.com>",
  "dependencies": {
    "gatsby": "^2.14.6",
    "gatsby-link": "^2.2.10",
    "gatsby-plugin-google-analytics": "^2.1.26",
    "gatsby-plugin-react-helmet": "^3.1.5",
    "gatsby-remark-autolink-headers": "^2.1.16",
    "gatsby-remark-prismjs": "^3.3.9",
    "gatsby-source-filesystem": "^2.1.18",
    "gatsby-transformer-remark": "^2.6.19",
    "prismjs": "^1.17.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-headroom": "^3.0.0",
    "react-helmet": "^5.2.1"
  },
  "keywords": [
    "gatsby",
    "blog"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build:travis": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write  --print-width 80 --tab-width 4 --no-semi --single-quote --trailing-comma all --arrow-parens always",
      "git add"
    ]
  },
  "devDependencies": {
    "husky": "^3.0.4",
    "lint-staged": "^9.2.5",
    "prettier": "^2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zachlysobey/personal-site"
  }
}
